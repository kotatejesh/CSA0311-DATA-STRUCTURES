#include <stdio.h>
void main()
{
	int i,n;
	printf("enter the number of elements in array");
	scanf("%d",&n);
	int arr[n];
	printf("enter the %d elements in array",n);
	for(int i=0;i<n;i++)
	{
		scanf("%d",&arr[i]);
	}
	printf("even numbers in an array");
	for(int i=0;i<n;i++)
	{
		if(arr[i]%2==0)
		printf("%d",arr[i]);
	}
	printf("\nodd numbers in array ");
	for(int i=0;i<n;i++){
		if(arr[i]%2==1)
		printf("%d",arr[i]);
	}
}
